@media (min-height: 40.125em) {
  .picker--time .picker__box {
    margin-bottom: 5em;
  }
}

.modal
    .picker--opened 
        top 0
        left 0
        right 0
        .picker__holder
            background none
            position absolute
            top 0
            left .5rem
            min-width 302px
            height 310px 
            overflow hidden
            // transform translate(0,-20%)
            .picker__frame
                top 0
                margin 0
                .picker__wrap
                    .picker__box
                        border 1px solid primary-color
                        background lightness(grey, 95)
                        border-radius 0 1rem 1rem 1rem
                        .picker__date-display
                            display none
                        .picker__footer
                            display none